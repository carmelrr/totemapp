{
  "expo": {
    "name": "Climbing Gym App",
    "slug": "climbing-gym-app",
    "scheme": "climbing-gym-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "jsEngine": "hermes",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourdomain.climbinggymapp",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "אפליקציה זו זקוקה לגישה לגלריה כדי להעלות תמונות של קירות טיפוס"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.yourdomain.climbinggymapp",
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "אפליקציה זו זקוקה לגישה לגלריה כדי להעלות תמונות של קירות טיפוס"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "314d40f3-b56b-4919-9dfe-817d3f8a0540"
      },
      "expoClientId": "720872675049-lpr4j4qvprtk8g19l36tr5qla8ebs33l.apps.googleusercontent.com",
      "androidClientId": "720872675049-ubhok243tvh36cut4ptchnubsckulj04.apps.googleusercontent.com",
      "iosClientId": "720872675049-e0du1dtbe53puak85sr4da6c8i0gjhak.apps.googleusercontent.com"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/314d40f3-b56b-4919-9dfe-817d3f8a0540"
    }
  }
}
